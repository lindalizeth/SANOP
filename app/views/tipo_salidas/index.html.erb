<h1>Listado tipo de salidas</h1>

<%= form_tag tipo_salidas_path, :method => 'get' do %> 
<% content_tag :label do %> 
Search term: 
<%= text_field_tag :search, params[:search] %> 
<%= submit_tag "Buscar", :name => nil %> 
<% end %> 
<% end %> 

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_salidas.each do |tipo_salida| %>
      <tr>
        <td><%= tipo_salida.nombre %></td>
        <td><%= link_to 'Ver', tipo_salida %></td>
        <td><%= link_to 'Editar', edit_tipo_salida_path(tipo_salida) %></td>
        <td><%= link_to 'Eliminar', tipo_salida, method: :delete, data: { confirm: 'Esta seguro de querer eliminar esto?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Tipo de salida', new_tipo_salida_path %>

<%= will_paginate @clientes, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente 
&raquo;' %> 
<br> 
Tipo_salida: <%= @tipo_salidas.count %> 
