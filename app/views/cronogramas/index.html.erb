<h1>Listing cronogramas</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Lugar salida</th>
      <th>Ficha grupo</th>
      <th>Tipo salida</th>
      <th>Programa</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cronogramas.each do |cronograma| %>
      <tr>
        <td><%= cronograma.fecha %></td>
        <td><%= cronograma.lugar_salida %></td>
        <td><%= cronograma.ficha_grupo %></td>
        <td><%= cronograma.tipo_salida %></td>
        <td><%= cronograma.programa %></td>
        <td><%= cronograma.descripcion %></td>
        <td><%= link_to 'Show', cronograma %></td>
        <td><%= link_to 'Edit', edit_cronograma_path(cronograma) %></td>
        <td><%= link_to 'Destroy', cronograma, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cronograma', new_cronograma_path %>
